{
	"name": "flipboxfactory/organizations",
	"description": "Simple parent + child management",
	"version": "3.0.0",
	"type": "craft-plugin",
	"keywords": [
		"flipbox",
		"organization",
		"parent-child",
		"craftcms",
		"yii2"
	],
	"license": "proprietary",
	"authors": [{
		"name": "Flipbox Factory",
		"email": "hello@flipboxfactory.com"
	}],
	"support": {
		"email": "support@flipboxfactory.com",
		"issues": "https://github.com/flipboxfactory/organizations/issues?state=open",
		"source": "https://github.com/flipboxfactory/organizations",
		"docs": "https://github.com/flipboxfactory/organizations",
		"rss": "https://github.com/flipboxfactory/organizations/commits/master.atom"
	},
	"require": {
		"craftcms/cms": "^3.2",
		"flipboxfactory/craft-ember": "^2.4.1",
		"flipboxfactory/craft-elements-nested-index": "^2.3.1",
		"tightenco/collect": "^5.5"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.4",
		"codeception/codeception": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"flipbox\\organizations\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"flipbox\\organizations\\tests\\": "tests/src/"
		}
	},
	"extra": {
		"name": "Organizations",
		"handle": "organizations",
		"class": "flipbox\\organizations\\Organizations",
		"developer": "Flipbox Digital",
		"developerUrl": "https://www.flipboxfactory.com",
		"changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/organizations/master/CHANGELOG.md",
		"documentationUrl": "https://organizations.flipboxfactory.com",
		"hasCpSettings": true,
		"hasCpSection": true,
		"schemaVersion": "1.3.1",
		"branch-alias": {
			"dev-develop": "3.0.x-dev",
			"dev-master": "3.0.x-dev"
		}
	}
}
